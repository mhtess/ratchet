@article{Kirby2001,
abstract = {A computationally implemented model of the transmission of$\backslash$nlinguistic behavior over time is presented. In this iterated learning$\backslash$nmodel (ILM), there is no biological evolution, natural selection, nor$\backslash$nany measurement of the success of the agents at communicating (except$\backslash$nfor results-gathering purposes). Nevertheless, counter to intuition,$\backslash$nsignificant evolution of linguistic behavior is observed. From an$\backslash$ninitially unstructured communication system (a protolanguage), a fully$\backslash$ncompositional syntactic meaning-string mapping emerges. Furthermore,$\backslash$ngiven a nonuniform frequency distribution over a meaning space and a$\backslash$nproduction mechanism that prefers short strings, a realistic$\backslash$ndistribution of string lengths and patterns of stable irregularity$\backslash$nemerges, suggesting that the ILM is a good model for the evolution of$\backslash$nsome of the fundamental features of human language},
