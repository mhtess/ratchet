@article{Perreault2012,
abstract = {There has been much interest in understanding the evolution of social learning. Investigators have tried to understand when natural selection will favor individuals who imitate others, how imitators should deal with the fact that available models may exhibit different behaviors, and how social and individual learning should interact. In all of this work, social learning and individual learning have been treated as alternative, conceptually distinct processes. Here we present a Bayesian model in which both individual and social learning arise from a single inferential process. Individuals use Bayesian inference to combine social and nonsocial cues about the current state of the environment. This model indicates that natural selection favors individuals who place heavy weight on social cues when the environment changes slowly or when its state cannot be well predicted using nonsocial cues. It also indicates that a conformist bias should be a universal aspect of social learning. ?? 2012 Elsevier Inc.},
