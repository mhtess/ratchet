journal = {Behavioral and Brain Sciences},
keywords = {additive clusteriiig,categorizatioii,coiitrast niodel,concept leariiirig,featiires,gerieralization,il spice,logic,nayesiaii iiifereiice,psyclio-,similiirity},
pages = {629--640},
